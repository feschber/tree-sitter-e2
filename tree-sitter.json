{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "e2",
      "camelcase": "E2",
      "title": "E2",
      "scope": "source.e2",
      "file-types": ["e2"],
      "injection-regex": "^e2$",
      "class-name": "TreeSitterE2"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "E2 grammar for tree-sitter",
    "authors": [
      {
        "name": "Ferdinand Schober",
        "email": "ferdinandschober20@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/feschber/tree-sitter-e2"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
